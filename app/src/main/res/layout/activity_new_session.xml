<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/relative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:gravity="start"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.sce.tom.physotrack.NewSession"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:orientation="vertical"
    android:layoutDirection="ltr"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layoutDirection="ltr"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_kiss_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_kiss_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_kiss_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_ikiss"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="KissInfo_Button_Clicked"/>

        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_smile_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_smile_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_session_smile_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_ismile"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="SmileInfo_Button_Clicked"/>
        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_eyeClose_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_eyeClose_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_eyes_close_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_ieyeclose"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="EyeCloseInfo_Button_Clicked"/>
        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_blankly_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_blankly_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_blankly_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_iblankly"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="BlanklyInfo_Button_Clicked"/>
        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_rabbit_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_rabbit_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_rabbit_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_irabbit"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="RabbitInfo_Button_Clicked"/>
        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="10dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_browLifts_photo"
                android:layout_width="290dp"
                android:layout_height="52dp"
                android:layout_weight="1"
                android:background="@drawable/buttonshape"
                android:onClick="btn_browLifts_photo_On_click"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_eyebrow_lifts_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp" />

            <Space
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_weight="1" />

            <Button
                android:id="@+id/btn_ibrowlift"
                android:layout_width="35dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/info_logo"
                android:onClick="BrowLiftInfo_Button_Clicked"/>

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/relative2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

        </RelativeLayout>

    </LinearLayout>
</RelativeLayout>