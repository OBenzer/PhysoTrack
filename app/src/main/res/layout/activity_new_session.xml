<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relative"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:gravity="start"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.sce.tom.physotrack.NewSession"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:orientation="vertical"
    android:layoutDirection="ltr" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layoutDirection="ltr"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/blankly_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_blankly_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-40dp"
                android:onClick="BlanklyInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_blankly_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_blankly_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_blankly_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_blankly_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr" />

        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="15dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/browLift_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_eyebrow_lifts_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-15dp"
                android:onClick="BrowLiftInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_browLifts_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_browLifts_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_browLifts_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_browLifts_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr"/>

        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="15dp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/eyeClose_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_eyes_close_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-20dp"
                android:onClick="EyeCloseInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_eyeClose_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_eyeClose_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_eyeClose_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_eyeClose_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr" />

        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="15dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/rabbit_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_rabbit_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-40dp"
                android:onClick="RabbitInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_rabbit_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_rabbit_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_rabbit_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_rabbit_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr" />

        </LinearLayout>


        <Space
            android:layout_width="10dp"
            android:layout_height="15dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/smile_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_session_smile_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-40dp"
                android:onClick="SmileInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_smile_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_smile_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_smile_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_smile_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr" />

        </LinearLayout>

        <Space
            android:layout_width="10dp"
            android:layout_height="15dp" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/kiss_tv"
                android:layout_width="180dp"
                android:layout_height="50dp"
                android:background="@drawable/buttonshape"
                android:gravity="center_vertical|center_horizontal"
                android:shadowColor="#2328A8"
                android:shadowDx="-0"
                android:shadowDy="0"
                android:shadowRadius="0"
                android:text="@string/New_Session_kiss_btn"
                android:textColor="#D3DCE0"
                android:textSize="15sp"
                android:padding="15dp"
                android:drawableStart="@android:drawable/ic_menu_info_details"
                android:drawablePadding="-40dp"
                android:onClick="KissInfo_Button_Clicked" />

            <Space
                android:layout_width="25dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_kiss_photo"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/camera_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_kiss_photo_On_click"
                android:contentDescription="@string/camerabutton_descr" />

            <Space
                android:layout_width="20dp"
                android:layout_height="50dp" />

            <ImageButton
                android:id="@+id/btn_kiss_gallery"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/gallery_btn"
                android:gravity="center_vertical|center_horizontal"
                android:onClick="btn_kiss_gallery_On_click"
                android:contentDescription="@string/gallerybutton_descr" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/relative2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

        </RelativeLayout>

    </LinearLayout>

    <Button
        android:id="@+id/btn_submit"
        android:layout_width="290dp"
        android:layout_height="52dp"
        android:background="@drawable/buttonshape"
        android:onClick="btn_submit_On_click"
        android:shadowColor="#2328A8"
        android:shadowDx="-0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="@string/submit_btn"
        android:textColor="#D3DCE0"
        android:textSize="15sp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/btn_showlandmarks"
        android:layout_width="290dp"
        android:layout_height="52dp"
        android:background="@drawable/buttonshape"
        android:onClick="btn_showLandmarks_On_click"
        android:shadowColor="#2328A8"
        android:shadowDx="-0"
        android:shadowDy="0"
        android:shadowRadius="0"
        android:text="@string/btn_showlandm"
        android:textColor="#D3DCE0"
        android:textSize="15sp"
        android:layout_marginBottom="12dp"
        android:layout_above="@+id/btn_submit"
        android:layout_alignStart="@+id/btn_submit" />

</RelativeLayout>
